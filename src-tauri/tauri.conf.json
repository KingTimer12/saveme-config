{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "saveme-config",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "beforeDevCommand": "bun dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Save me config!",
        "width": 800,
        "height": 600,
        "decorations": true,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
